<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:state_pressed="true">
        <shape>
            <corners android:radius="10dp" />
            <gradient android:angle="270" android:endColor="#80000000" android:startColor="#80000000" />
            <stroke android:width="1dp" android:color="#80000000" />
        </shape>
	</item>
	<item>
        <shape>
            <corners android:radius="10dp" />
            <gradient android:angle="270"
                android:endColor="#005f66"
                android:startColor="#4bbec6" />
            <stroke android:width="1dp" android:color="#bffaff" />
        </shape>
	</item>
</selector>